{
  "extends": "./base.json",
  "compilerOptions": {
  "jsx": "preserve",
  "moduleResolution": "nodenext",
    "noEmit": true,
    "allowJs": true,
    "incremental": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}

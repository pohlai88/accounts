{
  "name": "@aibos/tokens",
  "version": "0.1.0",
  "dependencies": {
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "typescript": "^5.6.2",
    "@aibos/eslint-config": "workspace:*",
    "@aibos/vitest-config": "workspace:*",
    "@aibos/prettier-config": "workspace:*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./tailwind.preset.cjs": "./tailwind.preset.cjs"
  },
  "files": [
    "dist/",
    "tailwind.preset.cjs"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint .",
    "typecheck": "tsc -p . --noEmit",
    "dev": "tsc -p . --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "type": "module"
}

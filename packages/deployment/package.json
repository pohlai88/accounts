{
  "name": "@aibos/deployment",
  "version": "1.0.0",
  "description": "Production deployment utilities and configurations for AI-BOS Accounts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p . --noEmit",
    "dev": "tsc -p . --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy:staging": "node dist/scripts/deploy-staging.js",
    "deploy:production": "node dist/scripts/deploy-production.js",
    "health-check": "node dist/scripts/health-check.js",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint .",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aibos/cache": "workspace:*",
    "@aibos/monitoring": "workspace:*",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4",
    "@aibos/eslint-config": "workspace:*",
    "@aibos/vitest-config": "workspace:*",
    "@aibos/prettier-config": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}

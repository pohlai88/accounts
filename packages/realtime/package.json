{
  "name": "@aibos/realtime",
  "version": "0.1.0",
  "description": "Real-time features and WebSocket integration for AI-BOS Accounts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p . --noEmit",
    "dev": "tsc -p . --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "dependencies": {
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/ws": "^8.5.10",
    "typescript": "^5.6.2",
    "@aibos/eslint-config": "workspace:*",
    "@aibos/vitest-config": "workspace:*",
    "@aibos/prettier-config": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}

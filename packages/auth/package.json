{
  "name": "@aibos/auth",
  "version": "0.1.0",
  "dependencies": {
    "@aibos/contracts": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aibos/eslint-config": "workspace:*",
    "@aibos/prettier-config": "workspace:*",

    "tsup": "^8.5.0",
    "typescript": "^5.6.2"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types-entry.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "tsc -p tsconfig.build.json",
    "build": "tsup",
    "lint": "eslint .",
    "typecheck": "tsc -p . --noEmit",
    "dev": "tsc -p . --watch",


    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "sideEffects": false,
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "type": "module"
}

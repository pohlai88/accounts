{
  "name": "@aibos/contracts",
  "version": "0.1.0",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aibos/eslint-config": "workspace:*",
    "@aibos/prettier-config": "workspace:*",
    "@aibos/vitest-config": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./db/period": {
      "types": "./dist/db/period.d.ts",
      "import": "./dist/db/period.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -p . --watch",
    "lint": "eslint .",
    "typecheck": "tsc -p . --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "type": "module"
}

{
  "name": "@aibos/api-gateway",
  "version": "1.0.0",
  "description": "API Gateway with centralized routing and rate limiting for AI-BOS Accounts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p . --noEmit",
    "dev": "tsc -p . --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint .",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aibos/auth": "workspace:*",
    "@aibos/cache": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/node": "^24.3.3",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4",
    "@aibos/eslint-config": "workspace:*",
    "@aibos/vitest-config": "workspace:*",
    "@aibos/prettier-config": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}

{
  "name": "@aibos/cache",
  "version": "1.0.0",
  "description": "Caching system with Redis support for AI-BOS Accounts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -p . --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint .",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "dependencies": {
    "ioredis": "^5.7.0",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4",
    "@aibos/eslint-config": "workspace:*",
    "@aibos/vitest-config": "workspace:*",
    "@aibos/prettier-config": "workspace:*"
  }
}
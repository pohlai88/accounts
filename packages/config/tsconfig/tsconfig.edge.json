{
  "extends": "./base.json",
  "compilerOptions": {
  "lib": ["ESNext", "WebWorker", "WebWorker.Iterable"],
  "target": "ESNext",
  "module": "ESNext",
  "moduleResolution": "nodenext",
    "outDir": "dist",
    "declaration": false,
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}

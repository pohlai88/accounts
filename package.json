{
  "name": "@aibos/monorepo",
  "devDependencies": {
    "@axiomhq/js": "^1.3.1",
    "@axiomhq/winston": "^1.3.1",
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@manypkg/cli": "^0.25.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "doctoc": "^2.2.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "lychee": "^0.2.12",
    "markdown-link-check": "^3.11.2",
    "markdownlint-cli": "^0.45.0",
    "markdownlint-cli2": "^0.18.1",
    "npm-check-updates": "^18.1.1",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "remark-cli": "^12.0.1",
    "remark-gfm": "^4.0.1",
    "remark-preset-lint-recommended": "^7.0.1",
    "remark-validate-links": "^13.1.0",
    "syncpack": "^13.0.4",
    "turbo": "^2.1.3",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.8.1",
    "vitepress": "^1.6.2",
    "vitest": "^2.1.9",
    "write-good": "^1.0.8",
    "zod": "4.0.0-beta.20250424T163858"
  },
  "engines": {
    "node": ">=20.12.0 <23.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.7.0",
  "pnpm": {
    "overrides": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "zod": "4.0.0-beta.20250424T163858",
      "typescript": "^5.6.2",
      "next": "^14.2.32",
      "drizzle-orm": "^0.33.0",
      "vitest": "^2.1.9",
      "winston": "^3.10.0",
      "vue": "3.5.11",
      "@vue/compiler-sfc": "3.5.11",
      "@vue/runtime-dom": "3.5.11",
      "@vue/runtime-core": "3.5.11",
      "@vue/shared": "3.5.11"
    }
  },
  "private": true,
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-gfm",
      "remark-validate-links"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "check:styles": "grep -R \"#[0-9A-Fa-f]\\{3,6\\}\" --exclude-dir=node_modules --include='*.{ts,tsx}' -n . && echo '❌ Raw hex found' && exit 1 || echo '✅ No raw hex'",
    "check:tokens": "node scripts/check-token-compliance.js",
    "check:tw-arbitrary": "grep -R \"\\[[^\\]]\\+\\]\" --include='*.{ts,tsx}' -n . && echo '❌ Arbitrary Tailwind value found' && exit 1 || echo '✅ No arbitrary values'",
    "deps:audit": "pnpm audit --audit-level moderate",
    "deps:check": "node scripts/check-internal-workspace.js && pnpm -w install --frozen-lockfile && pnpm deps:lint && pnpm -w dedupe --check",
    "deps:fix": "node scripts/syncpack-wrapper.js fix-mismatches && pnpm install",
    "deps:lint": "node scripts/syncpack-wrapper.js lint",
    "deps:list": "node scripts/syncpack-wrapper.js list",
    "deps:ranges": "node scripts/syncpack-wrapper.js set-semver-ranges --dependency-types dev,prod,peer,optional --semver-range ^",
    "deps:update": "node scripts/syncpack-wrapper.js update --dependency-types dev,prod",
    "deps:list-workspace": "syncpack list --filter @aibos",
    "deps:upgrade": "pnpm -w up -L",
    "deps:doctor": "node scripts/deps-doctor.js all",
    "deps:doctor-refs": "node scripts/deps-doctor.js workspace-refs",
    "deps:doctor-missing": "node scripts/deps-doctor.js missing-packages",
    "deps:doctor-list": "node scripts/deps-doctor.js list-packages",
    "deps:manypkg-check": "manypkg check",
    "deps:manypkg-fix": "manypkg fix",
    "deps:ncu-check": "npx ncu --target minor",
    "deps:ncu-update": "npx ncu --target minor -u && pnpm install",
    "dev": "turbo run dev",
    "docs:api": "node -e \"require('child_process').spawn('typedoc', [], {stdio:'inherit', shell:true}).on('exit', c=>{ if(c){ console.log('\\nℹ️ TypeDoc failed (likely TS build errors). API docs placeholders remain.'); process.exit(0);} })\"",
    "docs:build": "pnpm docs:guides:stubs && pnpm -C docs run build",
    "docs:build:ci": "pnpm docs:guides:stubs && pnpm docs:validate:internal && pnpm docs:lint:md && pnpm -C docs run build",
    "docs:check": "pnpm docs:lint && pnpm docs:remark",
    "docs:dev": "vitepress dev docs",
    "docs:doctor": "node scripts/docs/doctor.mjs",
    "docs:fmt": "prettier -w \"**/*.{md,mdx}\"",
    "docs:health": "node docs/scripts/health-check.js",
    "docs:lint": "markdownlint-cli2 \"packages/**/*.md\" \"apps/**/*.md\" \"tests/**/*.md\" \"*.md\" --ignore node_modules --ignore .git --ignore dist --ignore .next --ignore out",
    "docs:lint:md": "node scripts/docs/run.mjs markdownlint -c .markdownlint.jsonc \"docs/**/*.md\" \"docs/**/*.mdx\" README.md --ignore \"**/node_modules/**\"",
    "docs:lint:grammar": "node scripts/docs/grammar-check.mjs",
    "docs:links:external": "npx lychee --config ./.lychee.toml docs/ README.md || true",
    "docs:preview": "vitepress preview docs",
    "docs:remark": "remark . -q -f",
    "docs:toc": "doctoc . --notitle --maxlevel 3",
    "docs:validate:internal": "node scripts/docs/validate-internal-refs.mjs",
    "docs:guides:stubs": "node scripts/docs/create-missing-guides.mjs",
    "x": "node scripts/docs/run.mjs",
    "e2e": "turbo run e2e",
    "fix:aria": "node scripts/safe-fix.cjs --fix-aria",
    "fix:aria:write": "node scripts/safe-fix.cjs --fix-aria --write",
    "fix:safe": "node scripts/safe-fix.cjs",
    "fix:safe:write": "node scripts/safe-fix.cjs --write",
    "lint": "turbo run lint",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "postinstall": "turbo run build --filter='./packages/*' --force",
    "release": "changeset publish",
    "test": "turbo run test",
    "test:all": "pnpm test:unit && pnpm test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:d2-integration": "playwright test tests/e2e/d2-integration.spec.ts",
    "test:d2-performance": "k6 run tests/performance/d2-api.k6.js",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:fx-ingest": "node tests/fx-ingest-test.js",
    "test:performance": "k6 run tests/performance/journal-api.k6.js",
    "test:unit": "turbo run test",
    "test:unit:coverage": "turbo run test:coverage",
    "test:unit:watch": "turbo run test:watch",
    "test:vitest": "vitest run",
    "test:vitest:ui": "vitest --ui",
    "test:vitest:watch": "vitest",
    "typecheck": "turbo run typecheck",
    "version-packages": "changeset version"
  }
}
{
  "name": "@aibos/monorepo",
  "private": true,
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=20.12.0 <23.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "postinstall": "turbo run build --filter='./packages/*' --force",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "e2e": "turbo run e2e",
    "deps:lint": "syncpack lint",
    "deps:list": "syncpack list",
    "deps:list-workspace": "syncpack list --filter @aibos",
    "deps:fix": "syncpack fix-mismatches && pnpm install",
    "deps:check": "pnpm -w install --frozen-lockfile && pnpm deps:lint && pnpm -w dedupe --check",
    "deps:update": "pnpm -w up -L",
    "deps:audit": "pnpm audit --audit-level moderate",
    "test:unit": "turbo run test",
    "test:unit:watch": "turbo run test:watch",
    "test:unit:coverage": "turbo run test:coverage",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:vitest:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "k6 run tests/performance/journal-api.k6.js",
    "test:d2-performance": "k6 run tests/performance/d2-api.k6.js",
    "test:d2-integration": "playwright test tests/e2e/d2-integration.spec.ts",
    "test:fx-ingest": "node tests/fx-ingest-test.js",
    "test:all": "pnpm test:unit && pnpm test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.0",
    "syncpack": "^13.0.4",
    "turbo": "^2.1.3",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "zod": "^3.23.8",
      "typescript": "^5.6.2",
      "next": "14.2.32",
      "drizzle-orm": "^0.33.0",
      "vitest": "^2.0.5",
      "winston": "^3.10.0"
    }
  },
  "dependencies": {
    "@axiomhq/js": "^1.3.1",
    "@axiomhq/winston": "^1.3.1"
  }
}
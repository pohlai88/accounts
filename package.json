  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "turbo run format",
    "check": "pnpm lint && pnpm typecheck && pnpm test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:react": "vitest run --config vitest.config.react.ts",
    "test:react:watch": "vitest --config vitest.config.react.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "quality:check": "pnpm check && pnpm format:check && pnpm check:compliance",
    "quality:fix": "pnpm lint:fix && pnpm format && pnpm fix:all",
    "changeset": "changeset",
    "check:compliance": "node scripts/code-quality/check-token-compliance.js",
    "deps:audit": "pnpm audit --audit-level moderate",
    "deps:check": "node scripts/dependencies/check-internal-workspace.js && pnpm -w install --frozen-lockfile && pnpm deps:doctor && pnpm -w dedupe --check",
    "deps:doctor": "node scripts/dependencies/deps-doctor.js all",
    "deps:upgrade": "pnpm -w up -L",
    "deps:ncu-check": "pnpm dlx npm-check-updates --target minor",
    "deps:ncu-update": "pnpm dlx npm-check-updates --target minor -u && pnpm install",
    "deps:analyze": "pnpm -w dedupe --check",
    "deps:fix": "pnpm -w dedupe",
    "deps:report": "pnpm list --depth=0 && pnpm -w dedupe --check",
    "docs:types:emit": "pnpm -r --filter ./packages/* exec tsc -p tsconfig.json --emitDeclarationOnly --declaration --declarationMap false --noEmitOnError false",
    "docs:api": "pnpm build && typedoc",
    "docs:api:clean": "rimraf docs/api docs/api-src",
    "docs:api:src": "typedoc --options typedoc.src.json",
    "docs:api:watch": "typedoc --options typedoc.src.json --watch",
    "docs:api:serve": "pnpm docs:api:src && npx serve docs/api-src",
    "docs:coverage": "node scripts/doc-coverage.js",
    "docs:coverage:check": "pnpm docs:coverage",
    "docs:doctest": "node scripts/docs-doctest.js",
    "docs:search": "node scripts/doc-search.js",
    "docs:versioned": "node scripts/versioned-docs.js",
    "docs:full": "pnpm docs:coverage && pnpm docs:doctest && pnpm docs:search && pnpm docs:versioned",
    "docs:build": "nextra build docs",
    "docs:dev": "nextra dev docs",
    "docs:fmt": "prettier -w \"docs/**/*.{md,mdx}\"",
    "docs:preview": "nextra start docs",
    "docs:update-headers": "node scripts/document-manager.js update-headers",
    "docs:validate": "node scripts/document-manager.js validate",
    "docs:update-registry": "node scripts/document-manager.js update-registry",
    "docs:stats": "node scripts/document-manager.js stats",
    "docs:full-update": "node scripts/document-manager.js full-update",
    "docs:lint": "markdownlint '**/*.md' --ignore node_modules --ignore docs/api",
    "docs:spell-check": "write-good '**/*.md' --ignore node_modules --ignore docs/api",
    "docs:link-check": "markdown-link-check '**/*.md' --ignore node_modules --ignore docs/api",
    "docs:quality": "pnpm docs:lint && pnpm docs:spell-check && pnpm docs:validate",
    "e2e": "turbo run e2e",
    "fix:all": "node scripts/safe-fix.cjs --write",
    "fix:check": "node scripts/safe-fix.cjs",
    "lint:fix": "turbo run lint -- --fix",
    "format:check": "turbo run format:check",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "postinstall": "turbo run build --filter='./packages/*' --force",
    "release": "changeset publish",
    "version-packages": "changeset version"
  },
  "dependencies": {
    "@aibos/auth": "workspace:^0.1.0",

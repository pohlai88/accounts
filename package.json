  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "format": "turbo run format",
    "check": "pnpm lint && pnpm typecheck && pnpm test",
    "quality:check": "pnpm check && pnpm format:check && pnpm check:compliance",
    "quality:fix": "pnpm lint:fix && pnpm format && pnpm fix:all",
    "changeset": "changeset",
    "check:compliance": "node scripts/code-quality/check-token-compliance.js",
    "deps:audit": "pnpm audit --audit-level moderate",
    "deps:check": "node scripts/dependencies/check-internal-workspace.js && pnpm -w install --frozen-lockfile && pnpm deps:doctor && pnpm -w dedupe --check",
    "deps:doctor": "node scripts/dependencies/deps-doctor.js all",
    "deps:upgrade": "pnpm -w up -L",
    "deps:ncu-check": "pnpm dlx npm-check-updates --target minor",
    "deps:ncu-update": "pnpm dlx npm-check-updates --target minor -u && pnpm install",
    "deps:analyze": "pnpm -w dedupe --check",
    "deps:fix": "pnpm -w dedupe",
    "deps:report": "pnpm list --depth=0 && pnpm -w dedupe --check",
    "docs:types:emit": "pnpm -r --filter ./packages/* exec tsc -p tsconfig.json --emitDeclarationOnly --declaration --declarationMap false --noEmitOnError false",
    "docs:api": "typedoc",
    "docs:api:clean": "rimraf docs/api",
    "docs:api:src": "typedoc --options typedoc.src.json",
    "docs:build": "nextra build docs",
    "docs:dev": "nextra dev docs",
    "docs:fmt": "prettier -w \"docs/**/*.{md,mdx}\"",
    "docs:preview": "nextra start docs",
    "e2e": "turbo run e2e",
    "fix:all": "node scripts/safe-fix.cjs --write",
    "fix:check": "node scripts/safe-fix.cjs",
    "lint:fix": "turbo run lint -- --fix",
    "format:check": "turbo run format:check",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "postinstall": "turbo run build --filter='./packages/*' --force",
    "release": "changeset publish",
    "test:all": "pnpm test:unit && pnpm test:e2e",
    "test:d2-integration": "playwright test tests/e2e/d2-integration.spec.ts",
    "test:d2-performance": "k6 run tests/performance/d2-api.k6.js",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:fx-ingest": "node tests/fx-ingest-test.js",
    "test:performance": "k6 run tests/performance/journal-api.k6.js",
    "test:unit": "turbo run test:unit",
    "test:unit:acc": "vitest run tests/unit/accounting --run",
    "test:unit:acc:core": "vitest run tests/unit/accounting/payment-processing-enhanced.test.ts tests/unit/accounting/gl-posting-simple.test.ts --run",
    "test:unit:acc:fast": "vitest run tests/unit/accounting/contract-validation.test.ts --run",
    "test:unit:coverage": "turbo run test:coverage",
    "test:unit:watch": "turbo run test:watch",
    "test:supabase": "pnpm dlx vitest run tests/setup/ --config tests/setup/vitest.config.ts",
    "test:supabase:watch": "pnpm dlx vitest tests/setup/ --config tests/setup/vitest.config.ts",
    "test:supabase:ui": "pnpm dlx vitest --ui tests/setup/ --config tests/setup/vitest.config.ts",
    "test:database": "pnpm dlx vitest run tests/setup/database.test.ts --config tests/setup/vitest.config.ts",
    "test:auth": "pnpm dlx vitest run tests/setup/auth.test.ts --config tests/setup/vitest.config.ts",
    "test:api": "pnpm dlx vitest run tests/setup/api.test.ts --config tests/setup/vitest.config.ts",
    "test:storage": "pnpm dlx vitest run tests/setup/storage.test.ts --config tests/setup/vitest.config.ts",
    "test:realtime": "pnpm dlx vitest run tests/setup/realtime.test.ts --config tests/setup/vitest.config.ts",
    "test:integration": "pnpm dlx vitest run tests/setup/integration.test.ts --config tests/setup/vitest.config.ts",
    "test:integration:gateway": "pnpm dlx vitest run tests/integration/api-gateway.test.ts --config tests/integration/vitest.config.ts",
    "test:integration:cache": "pnpm dlx vitest run tests/integration/cache.test.ts --config tests/integration/vitest.config.ts",
    "test:integration:idempotency": "pnpm dlx vitest run tests/integration/idempotency.test.ts --config tests/integration/vitest.config.ts",
    "test:integration:e2e": "pnpm dlx vitest run tests/integration/end-to-end.test.ts --config tests/integration/vitest.config.ts",
    "test:integration:all": "pnpm dlx vitest run tests/integration/ --config tests/integration/vitest.config.ts",
    "test:integration:api-gateway": "pnpm dlx vitest run tests/integration/api-gateway-integration.test.ts --config tests/integration/vitest.config.ts",
    "test:integration:database": "pnpm dlx vitest run tests/integration/database-integration.test.ts --config tests/integration/vitest.config.ts",
    "test:integration:accounting": "pnpm dlx vitest run tests/integration/accounting-integration-seeded.test.ts --config tests/integration/vitest.config.ts",
    "test:integration:phase2": "pnpm test:integration:api-gateway && pnpm test:integration:database && pnpm test:integration:accounting",
    "test:error-codes": "node scripts/check-error-codes.mjs",
    "test:invariants": "vitest run tests/invariants --run",
    "test:mutation": "npx stryker run",
    "test:mutate:quick": "stryker run -c stryker.quick.cjs",
    "test:robustness": "pnpm test:error-codes && pnpm test:invariants && pnpm test:mutation",
    "test:verify-fast": "pnpm -w test:unit:acc:fast && pnpm -w test:invariants && pnpm -w test:error-codes",
    "test:verify-core": "pnpm -w test:unit:acc:core && pnpm -w test:invariants && pnpm -w test:error-codes",
    "test:flake-hunt": "for ($i=1; $i -le 10; $i++) { pnpm test:unit:acc:fast; if ($LASTEXITCODE -ne 0) { exit 1 } }",
    "version-packages": "changeset version"
  },
  "dependencies": {

{
  "name": "@aibos/worker",
  "version": "0.1.0",
  "dependencies": {
    "@aibos/accounting": "workspace:*",
    "@aibos/contracts": "workspace:*",
    "@aibos/db": "workspace:*",
    "@aibos/utils": "workspace:*",
    "drizzle-orm": "^0.33.0",
    "inngest": "^3.22.12",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/pg": "^8.15.5",
    "typescript": "^5.6.2",
    "@aibos/eslint-config": "workspace:*",
    "@aibos/vitest-config": "workspace:*",
    "@aibos/prettier-config": "workspace:*"
  },
  "private": true,
  "scripts": {
    "build": "tsc -p . --noEmit",
    "dev": "tsc -p . --watch",
